{
  "name": "httpx-server",
  "version": "0.0.0",
  "description": "Respond to encrypted and unencrypted HTTP/1.1 and HTTP/2 requests on the same port",
  "keywords": [
    "http",
    "http2",
    "h2c",
    "tls"
  ],
  "homepage": "https://github.com/vinsonchuong/httpx-server",
  "bugs": "https://github.com/vinsonchuong/httpx-server/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/httpx-server",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "dependencies": {},
  "devDependencies": {
    "ava": "^3.10.0",
    "get-stream": "^5.1.0",
    "got": "^11.4.0",
    "make-cert": "^1.1.0",
    "semantic-release": "^17.1.1",
    "ws": "^7.3.1",
    "xo": "^0.32.1"
  },
  "ava": {
    "verbose": true,
    "nonSemVerExperiments": {
      "likeAssertion": true
    }
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "ava/use-t-well": "off",
      "import/extensions": "off",
      "import/no-anonymous-default-export": "off",
      "import/no-useless-path-segments": "off",
      "unicorn/import-index": "off"
    }
  }
}
